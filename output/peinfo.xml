            <Text text="数据基址" padding="100,10,0,0" width="80"/>
            <Edit text="" name="pe_database" padding="0,5,10,0" width="100" bordersize="1"  bordercolor="0xffffc09f" focusbordercolor="0xff4cb4e7" align="right"/>
